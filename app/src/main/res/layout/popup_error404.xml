<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.johnta.homeworkappv2.activities.ScheduleActivity"
    android:background="#87ceeb">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <Button
            android:id="@+id/button11"
            android:layout_width="match_parent"
            android:layout_margin="5sp"
            android:textColor="@android:color/white"
            android:textSize="20sp"
            android:background="@drawable/settingsbutton1"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="248dp"
            android:text="OK" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="fill_parent"
            android:textSize="20sp"
            android:textColor="@android:color/black"
            android:gravity="center"
            android:layout_height="wrap_content"

            android:text="Error 404"
            android:layout_above="@+id/button11"
            android:layout_centerHorizontal="true"
            android:onClick="onClickReturn"/>

        <Button
            android:id="@+id/button12"
            android:layout_width="match_parent"
            android:background="@drawable/settingsbutton1"
            android:layout_margin="5sp"
            android:textSize="20sp"
            android:textColor="@android:color/white"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button11"
            android:layout_centerHorizontal="true"
            android:text="ENTER NEW URL"
            android:onClick="newURL"/>


    </RelativeLayout>
</android.support.constraint.ConstraintLayout>